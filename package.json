{
  "name": "rick-and-morty-react",
  "author": "Camilo Arce (https://github.com/camiloarce)",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "bootstrap": "yarn install  && nps bootstrap",
    "regenerate": "lerna clean --yes && rm -rf ./node_modules  && yarn bootstrap",
    "lint": "nps lint",
    "test": "nps test"
  },
  "devDependencies": {
    "babel-eslint": "9.0.0",
    "eslint": "5.12.0",
    "eslint-config-standard": "12.0.0",
    "eslint-config-standard-react": "7.0.2",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jest": "22.3.0",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-standard": "4.0.0",
    "jest": "23.6.0",
    "lerna": "2.10.1",
    "nodemon": "1.18.4",
    "snazzy": "8.0.0",
    "standard": "12.0.1"
  },
  "dependencies": {
    "dotenv": "6.1.0",
    "nps": "5.9.3",
    "nps-utils": "1.7.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "__ENV__",
      "FormData",
      "fetch",
      "Headers",
      "it"
    ]
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  }
}
